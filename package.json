{
  "name": "x-plane-map-electron",
  "productName": "X-Plane-Map",
  "description": "Electron revamp of X-Plane-Map",
  "version": "2.0.0",
  "author": "Foucauld Degeorges <foucauld.degeorges@gmail.com>",
  "homepage": "https://github.com/foucdeg/x-plane-map-electron/",
  "license": "Beerware",
  "repository": {
    "type": "git",
    "url": "git://github.com/foucdeg/x-plane-map-electron.git"
  },
  "main": "app/background.js",
  "build": {
    "appId": "net.fouc.x-plane-map",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "./node_modules/.bin/webpack",
    "prerelease": "npm run build --env=production",
    "compile": "electron-builder",
    "start": "./node_modules/.bin/electron .",
    "watch": "./node_modules/.bin/webpack --watch"
  },
  "dependencies": {
    "about-window": "^1.7.0",
    "body-parser": "^1.17.2",
    "electron-context-menu": "^0.8.0",
    "electron-default-menu": "^1.0.1",
    "electron-settings": "^2.2.4",
    "express": "^4.15.4",
    "fs-jetpack": "^0.10.2",
    "geolib": "^2.0.23",
    "google-maps": "^3.2.1",
    "ip": "^1.1.5",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "qrcode-js": "0.0.2"
  },
  "devDependencies": {
    "css-loader": "^0.28.4",
    "electron": "^1.7.6",
    "electron-builder": "^19.19.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "minimist": "^1.2.0",
    "source-map-support": "^0.4.15",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.2"
  }
